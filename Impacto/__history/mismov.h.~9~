#ifndef _MISMOV_
#define _MISMOV_

#include "tarjeta.h" 
#use delay (clock=48000000)

void IMPACTO(){
   M3_H();
   M4_H();
   delay_ms(250);
   M3_P();
   M4_P();
   delay_ms(700);
   M3_A();
   M4_A();
   delay_ms(250);
   M3_P();
   M4_P();
   delay_ms(500);
}

void IMPACTO_PRESION(){
   M1_H();
   M2_H();
   M3_H();
   M4_H();
   delay_ms(250);
   M3_P();
   M4_P();
   delay_ms(700);
   M3_A();
   M4_A();
   delay_ms(250);
   M3_P();
   M4_P();
   delay_ms(500);
}

#endif
